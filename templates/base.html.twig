<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}REDA'ACT{% endblock %}</title>
		
		{# Google font #}
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Lily+Script+One&family=Playwrite+AU+VIC+Guides&display=swap" rel="stylesheet">
        
		{% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
		
		<link rel="stylesheet" href="{{ asset('/assets/css/home.css') }}">
		{# Font-awesome & Bootstrap CSS #}
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" rel="stylesheet">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.0/font/bootstrap-icons.css">
		<link rel="stylesheet" href="{{ asset('/assets/css/neumorphism.css') }}">
        {% endblock %}
		{% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        <script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
        {% endblock %}
    </head>
    <body class="bodycontent">
        {% block body %}{% endblock %}
    </body>
</html>